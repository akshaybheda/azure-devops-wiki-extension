{
    "manifestVersion": 1,
    "id": "list-followed-wikis",
    "publisher": "akshay-bheda",
    "version": "1.0.25",
    "name": "List Followed Wikis",
    "description": "This extension lists the wikis that the current user is following.",
    "public": false,
    "categories": [
        "Azure Boards"
    ],
    "icons": {
        "default": "my-ado-extension/src/images/icon.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "scopes":[
        "vso.notification",
        "vso.wiki"

    ],
    "contributions": [
        {
            "id": "list-followed-wikis",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "List Followed Wikis",
                "uri": "my-ado-extension/dist/index.html"
            }
        }
    ],
    "files": [
        {
            "path": "my-ado-extension/dist",
            "addressable": true
        }
    ]
}